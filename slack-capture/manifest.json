{
  "manifest_version": 3,
  "name": "Emotion icon on Slack",
  "version": "1.0",
  "description": "Slack上に感情のアイコンを表示します",
  "permissions": ["activeTab", "scripting"],
  "content_scripts": [
    {
      "matches": ["https://*.slack.com/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ],
  "icons": {
    "16": "icons/icon.jpg",
    "48": "icons/icon.jpg",
    "128": "icons/icon.jpg"
  }
}
